{
  "$jsonSchema": {
    "title": "Song",
    "bsonType": "object",
    "required": ["name", "releaseDate", "length", "genreId"],
    "properties": {
      "_id": {
      "bsonType": "objectId"
      },
      "name": {
      "bsonType": "string",
      "minLength": 2,
      "maxLength": 50
      },
      "length": {
      "bsonType": "int"
      },
      "releaseDate": {
      "bsonType": "date"
      },
      "genreId": {
      "bsonType": "objectId"
      },
      "artistId": {
      "bsonType": "objectId"
      },
      "albumId": {
      "bsonType": "objectId"
      }
    }
  }
}